@model _12DataMigration.Models.Student
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@using (Html.BeginForm())
{
    @Html.LabelFor(student => student.Name)
    @Html.TextBoxFor(student => student.Name)
    <br />

    @Html.LabelFor(student => student.Gender)
    @Html.TextBoxFor(student => student.Gender)
    <br />

    @Html.LabelFor(student => student.Email)
    @Html.TextBoxFor(student => student.Email)
    <br />

    @Html.LabelFor(student => student.MobileNo)
    @Html.TextBoxFor(student => student.MobileNo)
    <br />

    <input type="submit" value="Create" />


}

